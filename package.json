{
  "name": "chasing-the-kernel",
  "private": true,
  "type": "module",
  "scripts": {
    "----- dev -----": "",
    "dev": "pnpm run typegen && react-router dev",
    "----- build -----": "",
    "build:content-collections": "content-collections build --config ./app/lib/content-collections/index.ts",
    "build:react-router": "react-router build",
    "build": "pnpm run build:content-collections && pnpm run build:react-router",
    "preview": "pnpm run build && vite preview",
    "----- deploy -----": "",
    "deploy": "pnpm run build && wrangler deploy",
    "----- type generation, check -----": "",
    "cf-typegen": "wrangler types",
    "react-router-typegen": "react-router typegen",
    "typegen": "pnpm run cf-typegen && pnpm run react-router-typegen",
    "typecheck": "pnpm run typegen && tsc -b",
    "----- format, lint -----": "",
    "format": "biome format --write .",
    "lint": "biome lint --write .",
    "check": "biome check --write",
    "ci": "biome ci",
    "----- test -----": "",
    "test": "echo \"No tests yet\""
  },
  "dependencies": {
    "i18next": "^24.2.0",
    "i18next-browser-languagedetector": "^8.1.1",
    "i18next-http-backend": "^3.0.2",
    "isbot": "^5.1.27",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-i18next": "^15.2.0",
    "react-router": "^7.7.1",
    "remix-i18next": "^7.1.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.2",
    "@cloudflare/vite-plugin": "^1.0.12",
    "@content-collections/cli": "^0.1.7",
    "@content-collections/core": "^0.11.1",
    "@content-collections/markdown": "^0.1.4",
    "@content-collections/mdx": "^0.2.2",
    "@content-collections/remix-vite": "^0.2.2",
    "@react-router/dev": "^7.7.1",
    "@tailwindcss/vite": "^4.1.4",
    "@types/node": "^20.19.11",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "daisyui": "^5.0.54",
    "tailwindcss": "^4.1.4",
    "typescript": "^5.8.3",
    "vite": "^6.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.33.1",
    "zod": "^4.1.5"
  }
}
